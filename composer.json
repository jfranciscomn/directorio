{
	"name": "yiisoft/yii2-app-basic",
	"description": "Yii 2 Basic Application Template",
	"keywords": ["yii2", "framework", "basic", "application template"],
	"homepage": "http://www.yiiframework.com/",
	"type": "project",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/yiisoft/yii2/issues?state=open",
		"forum": "http://www.yiiframework.com/forum/",
		"wiki": "http://www.yiiframework.com/wiki/",
		"irc": "irc://irc.freenode.net/yii",
		"source": "https://github.com/yiisoft/yii2"
	},
	"minimum-stability": "beta",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*",
		"yiisoft/yii2-bootstrap": "*",
		"yiisoft/yii2-swiftmailer": "*",
		"kartik-v/yii2-widgets": "dev-master",
		"kartik-v/yii2-money": "dev-master",
		"kartik-v/bootstrap-checkbox-x": "dev-master",
		"kartik-v/yii2-checkbox-x": "dev-master",
		"kartik-v/yii2-grid": "dev-master",
		"kartik-v/yii2-helpers": "dev-master",
		"miloschuman/yii2-highcharts-widget": "*",
		"kartik-v/yii2-sortable": "dev-master",
		"kartik-v/yii2-field-range": "dev-master",
		"kartik-v/bootstrap-popover-x": "dev-master",
		"kartik-v/yii2-popover-x": "dev-master",
		"kartik-v/yii2-editable": "dev-master",
		"kartik-v/yii2-dynagrid": "dev-master",
		"kartik-v/yii2-builder": "dev-master",
		"kartik-v/yii2-detail-view": "dev-master",
		"kartik-v/yii2-label-inplace": "dev-master",
		"kartik-v/yii2-icons": "dev-master",
		"kartik-v/yii2-dropdown-x": "dev-master",
		"kartik-v/yii2-nav-x": "dev-master",
		"kartik-v/yii2-context-menu": "dev-master",
		"kartik-v/yii2-slider": "dev-master",
		"kartik-v/strength-meter": "dev-master",
		"kartik-v/yii2-password": "dev-master",
		"kartik-v/yii2-sortable-input": "dev-master",
		"kartik-v/yii2-markdown": "dev-master",
		"kartik-v/yii2-date-range": "dev-master",
		"kartik-v/php-date-formatter": "dev-master",
		"kartik-v/yii2-datecontrol": "dev-master"
	},
	"require-dev": {
		"yiisoft/yii2-codeception": "*",
		"yiisoft/yii2-debug": "*",
		"yiisoft/yii2-gii": "*"
	},
	"suggest": {
		"codeception/codeception": "Codeception, 1.8.*@dev is currently works well with Yii.",
		"codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
		"codeception/verify": "BDD Assertions for PHPUnit and Codeception"
	},
	"scripts": {
		"post-create-project-cmd": [
			"yii\\composer\\Installer::setPermission"
		]
	},
	"config": {
		"process-timeout": 1800
	},
	"extra": {
		"writable": [
			"runtime",
			"web/assets"
		],
		"executable": [
			"yii"
		]
	}
}
